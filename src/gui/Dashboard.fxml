          </items>
        </Menu>
        <Menu fx:id="menuBook" mnemonicParsing="false" text="Book">
          <items>
           
            <MenuItem mnemonicParsing="false" onAction="#addBookCopyEvent" text="Add Copy" />
          </items>
        </Menu>
        <Menu fx:id="menuCheckout" mnemonicParsing="false" text="Checkout Book" />
      </menus>
    </MenuBar>
  </top>
</BorderPane>
